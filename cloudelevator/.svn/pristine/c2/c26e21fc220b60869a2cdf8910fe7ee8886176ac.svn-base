	<sqlMap resource="sqlmaps/media-sqlmap.xml" />
	<sqlMap resource="sqlmaps/deviceMedia-sqlmap.xml" />
	<sqlMap resource="sqlmaps/notice-sqlmap.xml" />

</sqlMapConfig>